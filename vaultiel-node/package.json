{
  "name": "@vaultiel/node",
  "version": "0.1.0",
  "description": "Node.js bindings for Vaultiel - A library for Obsidian-style vaults",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "*.node"
  ],
  "napi": {
    "name": "vaultiel",
    "triples": {
      "defaults": true,
      "additional": [
        "aarch64-apple-darwin",
        "x86_64-apple-darwin",
        "x86_64-unknown-linux-gnu",
        "aarch64-unknown-linux-gnu",
        "x86_64-pc-windows-msvc"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0",
    "typescript": "^5.0.0"
  },
  "scripts": {
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "node test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lukas/vaultiel"
  },
  "keywords": [
    "obsidian",
    "markdown",
    "notes",
    "vault",
    "wikilinks",
    "napi-rs"
  ]
}
